{
  "name": "@didie/utils",
  "version": "1.0.17-pre",
  "description": "Utility Functions",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "src/**/*.d.ts",
    "src/**/*.js",
    "index.d.ts",
    "index.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jdiehl/utils.git"
  },
  "scripts": {
    "test": "tslint *.ts src/**.ts test/**.ts; jest",
    "watch": "jest --watch --testPathPattern",
    "build": "tsc --project tsconfig.json",
    "prepublishOnly": "npm test; npm run build",
    "bump": "npm-bump patch",
    "clean": "del index.js index.d.ts 'src/**/*.d.ts' 'src/**/*.js'"
  },
  "keywords": [],
  "author": "Jonathan Diehl",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.58",
    "del-cli": "^1.1.0",
    "jest": "^21.2.1",
    "npm-bump": "0.0.23",
    "ts-jest": "^21.2.4",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "skipBabel": true
      }
    },
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
