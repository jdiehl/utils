{
  "defaultSeverity": "error",
  "extends": [ "tslint:recommended" ],
  "jsRules": {},
  "rules": {
    "arrow-parens": false,
    "curly": false,
    "member-access": [ true, "no-public" ],
    "no-console": false,
    "no-empty-interface": false,
    "no-var-requires": false,
    "quotemark": [ true, "single", "jsx-double" ],
    "semicolon": [ true, "never" ],
    "trailing-comma": [ false ]
  },
  "rulesDirectory": []
}
